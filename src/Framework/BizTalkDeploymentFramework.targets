      Condition="'$(TargetsAfterLoadPropsFromEnvSettings)' == ''" />
    
    <MSBuild
      Projects="$(MSBuildProjectFullPath)"
      Properties="LoadedPropsFromEnvSettings=True;SkipExportSettings=True;$(PropsFromEnvSettingsDelimited)"
